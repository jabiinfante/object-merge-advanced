{
  "name": "object-merge-advanced",
  "version": "7.1.2",
  "description": "Recursive, deep merge of anything (objects, arrays, strings or nested thereof), which weighs contents by type hierarchy to ensure the maximum content is retained",
  "main": "index-es5.js",
  "scripts": {
    "build": "babel index.js --presets babel-preset-es2015 --out-file index-es5.js && babel util.js --presets babel-preset-es2015 --out-file util-es5.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test",
    "prepare": "npm run build",
    "test": "clear && ./node_modules/.bin/eslint index.js test.js && nyc --reporter=html --reporter=text ava && npm run build"
  },
  "repository": "codsen/object-merge-advanced",
  "keywords": [
    "ast",
    "plain",
    "object",
    "posthtml-tree",
    "posthtmltree",
    "merge",
    "objects",
    "advanced"
  ],
  "author": {
    "name": "Roy Revelt",
    "email": "roy@codsen.com",
    "url": "codsen.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codsen/object-merge-advanced/issues"
  },
  "homepage": "https://github.com/codsen/object-merge-advanced#readme",
  "dependencies": {
    "array-includes-all": "^1.0.0",
    "array-includes-with-glob": "^1.1.0",
    "check-types-mini": "^2.0.0",
    "lodash.clonedeep": "*",
    "type-detect": "*"
  },
  "devDependencies": {
    "ava": "*",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "clear-cli": "*",
    "coveralls": "*",
    "eslint": "^4.7.0",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "husky": "*",
    "nyc": "*"
  },
  "ava": {
    "verbose": true
  }
}
