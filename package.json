{
  "name": "object-merge-advanced",
  "version": "1.5.0",
  "description": "Like .merge but an array in a key's value trumps a plain object and a plain object trumps a string. Works for nested objects too.",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test && doctoc README.md",
    "test": "standard && nyc --reporter=html --reporter=text ava"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/code-and-send/object-merge-advanced.git"
  },
  "keywords": [
    "ast",
    "plain",
    "object",
    "posthtml-tree",
    "posthtmltree",
    "merge",
    "objects",
    "advanced"
  ],
  "author": "Roy Reveltas <roy@codeandsend.com> (http://codeandsend.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/code-and-send/object-merge-advanced/issues"
  },
  "homepage": "https://github.com/code-and-send/object-merge-advanced#readme",
  "dependencies": {
    "lodash.isplainobject": "*",
    "lodash.isstring": "*"
  },
  "devDependencies": {
    "ava": "*",
    "coveralls": "*",
    "doctoc": "*",
    "husky": "*",
    "nyc": "*",
    "standard": "*"
  },
  "ava": {
    "verbose": true
  }
}
